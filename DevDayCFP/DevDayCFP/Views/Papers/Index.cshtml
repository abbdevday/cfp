@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<System.Collections.Generic.IEnumerable<DevDayCFP.Models.Paper>>

<div class="section-header">
    <h1>Papers List</h1>
</div>
<div class="row">
    <div class="col-md-offset-1 col-md-10">
        <table class="table">
            <thead>
                <tr>
                    <th class="col-md-1">Lp.</th>
                    <th class="col-md-5">Title</th>
                    <th class="col-md-2">Status</th>
                    <th class="col-md-2">Last modification</th>
                    <th class="col-md-2">Actions</th>
                </tr>
            </thead>
            @foreach (var paper in Model)
            {
                <tr>
                    <td>1.</td>
                    <td>@paper.Title</td>
                    <td>@paper.LastModificationDate</td>
                    <td><a href="#">Edit</a></td>
                </tr>
            }
        </table>
    </div>
</div>
